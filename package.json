{
  "name": "webgazer",
  "version": "3.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/brownhci/WebGazer.git"
  },
  "license": "GPL-3.0-or-later",
  "main": "dist/webgazer.js",
  "module": "src/index.mjs",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "test": "./test/run_tests_and_server.sh",
    "dev": "webpack --progress --config webpack.config.js --mode=development",
    "build": "webpack --progress --config webpack.config.js --mode=production",
    "postbuild": "cp dist/webgazer.js www/. && cp dist/webgazer.js www/data/src/. && cp dist/webgazer.js.map www/. && cp dist/webgazer.js.map www/data/src/.",
    "gen_docs": "jsdoc -c jsdoc.conf.json src/*"
  },
  "files": [
    "/dist",
    "/src"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run build",
      "post-checkout": "yarnhook && npm --prefix ./www install ./www/ && npm run build",
      "post-merge": "yarnhook && npm --prefix ./www install ./www/ && npm run build",
      "post-rewrite": "yarnhook && npm --prefix ./www install ./www/ && npm run build"
    }
  },
  "keywords": [
    "webgazer",
    "eyetracking",
    "webcam"
  ],
  "dependencies": {
    "@tensorflow-models/face-landmarks-detection": "0.0.3",
    "@tensorflow/tfjs": "^3.15.0",
    "localforage": "^1.10.0",
    "numeric": "^1.2.6",
    "regression": "^2.0.1"
  },
  "devDependencies": {
    "browser-sync": "^2.26.14",
    "chai": "^4.2.0",
    "file-loader": "^6.0.0",
    "husky": "^4.2.5",
    "jsdoc": "^3.6.5",
    "mocha": "^10.0.0",
    "ndb": "^1.1.5",
    "puppeteer": "^15.3.1",
    "rimraf": "^3.0.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "yarnhook": "^0.6.0"
  }
}
